import board
from time import sleep

import RPi.GPIO as GPIO        

# CODE FOR LED

myLED = 26

GPIO.setmode(GPIO.BCM) 			# initializes that 26 is the GPIO26 pin
GPIO.setup(myLED, GPIO.OUT)

while True:
	
	GPIO.output(myLED, GPIO.HIGH)
	print("HIGH")
	sleep(1)
	GPIO.output(myLED, GPIO.LOW)
	print("LOW")
	sleep(1)
	
GPIO.cleanup()
	
